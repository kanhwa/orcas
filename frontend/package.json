{
  "name": "orcas-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "_notes": "To start the dev server on port 5173, run 'npm run dev:5173'. This enforces --strictPort so the server will NOT auto-switch to 5174/5175 if 5173 is in use. If you see 'Port 5173 is in use', run: lsof -i :5173 && kill -9 <PID>, then retry npm run dev:5173",
  "scripts": {
    "dev": "vite --host localhost --port 5173 --strictPort",
    "dev:5173": "vite --host localhost --port 5173 --strictPort",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "lucide-react": "^0.562.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
